@model IEnumerable<AssetManagement.Models.UserStore>


@{
    Layout = null;
}

<style>

    *,
    *::before,
    *::after {
        box-sizing: border-box;
        -webkit-box-sizing: border-box;
    }



    body {
        font-family: 'Chivo', sans-serif;
        margin: 0;
        padding: 10px;
        height: 100vh;
    }

</style>
<h4>USER STORE TAKEN</h4>
<div class="table-responsive table-container">
    <table class="table" style="text-align: center;" id="TableId">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.User)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.Store)
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr style="text-align: center;">
                    <td>
                        @Html.DisplayFor(modelItem => item.User.UserFullName)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Store.StoreName)
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>

<script>

</script>