            </td>
            <td>
                @Html.DisplayFor(modelItem => item.type_status)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.type_createdby)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.type_createddt)
            </td>
