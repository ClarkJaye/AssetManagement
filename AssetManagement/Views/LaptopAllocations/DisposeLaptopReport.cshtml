@model IEnumerable<AssetManagement.Models.ReturnLTA>

@{
    ViewData["Title"] = "Report";
}

<div class="m-4">

    <div class="allocation-details">
        <h4 class="allocation-title">Disposed Laptops</h4>
    </div>
    <div class="report-view">
        <div class="report-item">
            <span class="report-label">Total Dispose Laptop:</span>
            <span class="report-value">@ViewBag.TotalDisposeLaptop</span>
        </div>
    </div>


    <div class="card shadow-sm card-selected">
        <div class="card-body">

            <div class="table-responsive">
                <table class="table table-striped table-datatable">
                    <thead>
                        <tr>
                            <th hidden>
                                @Html.DisplayNameFor(model => model.ReturnID)
                            </th>
                            <th>
                                ALLOCATION ID
                            </th>
                            <th>
                                LAPTOP CODE
                            </th>
                            <th>
                                LAPTOP DESCRIPTION
                            </th>
                            <th>
                                BRAND
                            </th>
                            <th>
                                LEVEL
                            </th>
                            <th>
                                OS
                            </th>
                            <th>
                                MEMORY
                            </th>
                            <th>
                                HARDDISK
                            </th>
                            <th>
                                PO
                            </th>
                            <th>
                                SERIAL
                            </th>
                            <th>
                                OWNER
                            </th>
                            <th>
                                COMPUTER NAME
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.ReturnType)
                            </th>
                            <th hidden>
                                @Html.DisplayNameFor(model => model.Status)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.DateCreated)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.DateUpdated)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.UserCreated)
                            </th>
                            <th>
                                @Html.DisplayNameFor(model => model.UserUpdated)
                            </th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td hidden>
                                    @Html.DisplayFor(modelItem => item.ReturnID)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.LaptopAllocation.AllocId)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.LaptopAllocation.LaptopInventoryDetails.laptoptinvCode)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.LaptopAllocation.LaptopInventoryDetails.LaptopInventory.Description)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.LaptopAllocation.LaptopInventoryDetails.LaptopInventory.Brand.BrandDescription)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.LaptopAllocation.LaptopInventoryDetails.LaptopInventory.Level.LevelDescription)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.LaptopAllocation.LaptopInventoryDetails.LaptopInventory.OS.OSDescription)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.LaptopAllocation.LaptopInventoryDetails.LaptopInventory.Memory.Capacity.CapacityDescription)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.LaptopAllocation.LaptopInventoryDetails.LaptopInventory.HardDisk.Capacity.CapacityDescription)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.LaptopAllocation.LaptopInventoryDetails.PO)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.LaptopAllocation.LaptopInventoryDetails.SerialCode)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.LaptopAllocation.OwnerCode)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.LaptopAllocation.ComputerName)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.ReturnType.Description)
                                </td>
                                <td hidden>
                                    @Html.DisplayFor(modelItem => item.Status.status_code)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.DateCreated)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.DateUpdated)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.UserCreated.UserCode)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.UserUpdated.UserCode)
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

