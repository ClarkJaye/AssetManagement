@model AssetManagement.Models.Ups

@{
    ViewData["Title"] = "Edit";
}

<style>
    .table tr td{
        vertical-align: top;
    }
</style>

<h4>Uninterruptible Power Supply</h4>
<hr />
<div>
    <table class="table">
        <tr>
            <td hidden>
                <label asp-for="ups_store" class="control-label"></label>
                @Model.ups_store
            </td>
            <td hidden>
                <label asp-for="ups_code" class="control-label"></label>
                @Model.ups_code
            </td>
            <td>
                <label asp-for="ups_warranty" class="control-label"><b>Warranty :</b></label>
                @Model.ups_warranty
            </td>
            <td>
                <label asp-for="ups_validity" class="control-label"><b>Validity :</b></label>
                @Model.ups_validity
            </td>
            <td>
                <label asp-for="ups_iovoltage" class="control-label"><b>IO Voltage :</b></label>
                @Model.ups_iovoltage
            </td>
            <td>
                <label asp-for="ups_dcvoltage" class="control-label"><b>DC Voltage :</b></label>
                @Model.ups_dcvoltage
            </td>
            <td>
                <label asp-for="ups_idealload" class="control-label"><b>Ideal Load :</b></label>
                @Model.ups_idealload
            </td>
            <td>
                <label asp-for="ups_currentload" class="control-label"><b>Current Load:</b></label>
                @Model.ups_currentload
            </td>
            <td>
                <label asp-for="ups_dtinstalled " class="control-label"><b>Date Installed :</b></label>
                @Model.ups_dtinstalled
            </td>
            <td>
                <label asp-for="ups_serviceyrs" class="control-label"><b>Service Years :</b></label>
                @Model.ups_serviceyrs
            </td>
            <td>
                <label asp-for="ups_lastpmdt" class="control-label"><b>Last PM :</b></label>
                @Model.ups_lastpmdt
            </td>
            <td>
                <label asp-for="ups_battrepcount" class="control-label"><b>Battery Count :</b></label>
                @Model.ups_battrepcount
            </td>
            <td>
                <label asp-for="ups_unitserve" class="control-label"><b>Unit Serve :</b></label>
                @Model.ups_unitserve
            </td>
            <td>
                <label asp-for="ups_remarks" class="control-label"><b>Remarks :</b></label>
                @Model.ups_remarks
            </td>
            <td>
                <label asp-for="ups_brand" class="control-label"><b>Brand :</b></label>
                @Model.Brand.BrandDescription
            </td>
            <td>
                <label asp-for="ups_model" class="control-label"><b>Model :</b></label>
                @Model.Model.ModelDescription
            </td>
            <td>
                <label asp-for="ups_type" class="control-label"><b>UPS Type :</b></label>
                @Model.UpsType.type_description
            </td>
            <td>
                <label asp-for="ups_vendor" class="control-label"><b>Vendor :</b></label>
                @Model.Vendor.VendorName
            </td>
            <td>
                <label asp-for="ups_condition" class="control-label"><b>UPS Condition :</b></label>
                @Model.UpsCondition.condition_description
            </td>
            <td>
                <label asp-for="ups_battstatus" class="control-label"><b>Battery Status :</b></label>
                @Model.UpsBattStatus.status_description
            </td>
        </tr>
    </table>
</div>
<hr />
<div>
    <form asp-action="Edit">
        <div class="row form-group">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="ups_store" />
            <div class="form-group col-2">
                <label asp-for="ups_store" class="control-label"></label>
                <input readonly style="background-color: white;" asp-for="ups_store" class="form-control" />
                <span asp-validation-for="ups_store" class="text-danger"></span>
            </div>
            <div class="form-group col-2">
                <label asp-for="ups_code" class="control-label"></label>
                <input readonly style="background-color: white;" asp-for="ups_code" class="form-control" />
                <span asp-validation-for="ups_code" class="text-danger"></span>
            </div>
            <div class="form-group col-2" hidden>
                <label asp-for="ups_brand" class="control-label"></label>
                <select asp-for="ups_brand" class="form-control" asp-items="ViewBag.ups_brand"></select>
